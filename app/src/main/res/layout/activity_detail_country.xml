<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context="com.youssary.countries.country.view.DetailCountry">
	<LinearLayout
		android:id="@+id/lydetail"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/tvTraslatorGermany">
		<include layout="@layout/adapter_list_account_content" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/lyMap"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/lydetail">
		<include layout="@layout/activity_maps" />
	</LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
